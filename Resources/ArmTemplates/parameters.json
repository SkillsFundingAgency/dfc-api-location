{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appSharedResourceGroup": {
      "value": "__appSharedResourceGroup__"
    },
    "appSharedStorageAccountName": {
      "value": "__appSharedStorageAccountName__"
    },
    "aspInstances": {
      "value": __aspInstances__
    },
    "aspSize": {
      "value": "__aspSize__"
    },
    "aspTier": {
      "value": "__aspTier__"
    },
    "aseHostingEnvironmentName": {
      "value": "__AseServiceName__"
    },
    "aseResourceGroup": {
      "value": "__aseResourceGroup__"
    },
    "environment": {
      "value": "__ReleaseEnvironmentName__",
      "metadata": {
        "comment": "Bug in tokenisation task prevents referencing Release.EnvironmentName directly.  https://github.com/TotalALM/VSTS-Tasks/issues/29"
      }
    },
    "FunctionAppInsightName": {
      "value": "__FunctionAppInsightName__"
    },
    "FunctionAppName": {
      "value": "__FunctionAppName__"
    },
    "enableAlerts": {
      "value": __EnableAzureMonitorAlerting__
    },
    "ExceptionCountThreshold": {
      "value": "__ExceptionCountThreshold__"
    },
    "MemoryPercentageThreshold": {
      "value": "__MemoryPercentageThreshold__"
    },
    "CPUPercentageThreshold": {
      "value": "__CPUPercentageThreshold__"
    },
    "apimLoggerName": {
      "value": "__ApimLoggerName__"
    },
    "apimResourceGroup": {
      "value": "__SharedResourceGroupName__"
    },
    "apimServiceName": {
      "value": "__ApimServiceName__"
    },
    "apimProductName": {
      "value": "__ApimProductInstanceName__"
    },
    "apimApiName": {
      "value": "__apimApiName__"
    },
    "AzureSearchEndpointUri": {
      "value": "__AzureSearchServiceUri__"
    },
    "AzureSearchSearchServiceName": {
      "value": "__AzureSearchServiceName__"
    },
    "AzureSearchSearchServiceAdminAPIKey": {
      "value": "__AzureSearchServiceAdminKey__"
    }
  }
}